{
    "name": "veloxia/markup",
    "description": "Schema markup for Laravel.",
    "keywords": [
        "veloxia",
        "markup"
    ],
    "homepage": "https://github.com/veloxiadev/markup",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Viktor Svensson",
            "email": "viktor@veloxia.se",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Veloxia\\Markup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Veloxia\\Markup\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Veloxia\\Markup\\MarkupServiceProvider"
            ],
            "aliases": {
                "Markup": "Veloxia\\Markup\\MarkupFacade"
            }
        }
    }
}